{
  "name": "oddones/php-goolab-api",
  "description": "Laravel PHP Facade/Wrapper for the GooLabApi",
  "keywords": ["GooLabApi", "api", "morphing", "laravel", "oddones"],
  "license": "MIT",
  "authors": [
    {
      "name": "OddOnes",
      "email": "info@oddones.jp"
    }
  ],
  "require": {
    "php": "^7.0|^8.0",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1|^9.3"
  },
  "autoload": {
    "psr-4": {
      "OddOnes\\GooLabApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OddOnes\\GooLabApi\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OddOnes\\GooLabApi\\GooLabApiServiceProvider"
      ],
      "aliases": {
        "GooLabApi": "OddOnes\\GooLabApi\\Facades\\GooLabApi"
      }
    }
  }
}
